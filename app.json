{
  "expo": {
    "name": "02-nativeapp",
    "slug": "02-nativeapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo/IchiryuRecipe_Favicon_256×256px.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo/IchiryuRecipe_tate.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/IchiryuRecipe_Favicon_256×256px.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "package": "host.exp.exponent"
    },
    "web": {
      "favicon": "./assets/logo/IchiryuRecipe_Favicon_32×32px.png"
    }
  }
}
